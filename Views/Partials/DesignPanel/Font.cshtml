                            <!-- Font type item -->
                            <label class="usn-font-type-item">
                                <input name="taboptions_{{$index}}" type="radio" value="google" ng-model="item.fontType" ng-click="updateFontFields($index,$event)">
                                <span></span>
                                <div class="inner">
