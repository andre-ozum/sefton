<div id="design-4" class="usn-cms-grouping usn-design-grouping usn-cms-fonts-panel">

    <!-- Group section -->
    <div class="usn-group-section">

        <label class="usn-label">Fonts</label>
        <span class="note usn-note">Select the fonts to be used across your site. You can choose between  System Fonts or Google Fonts.</span>

        <!-- Accordion -->
        <div class="usn-cms-accordion">

            <!-- Sortable -->
            <div ui-sortable ng-model="styles.themeFonts.fontItems">

                <!-- Sortable item -->
                <div ng-repeat="item in styles.themeFonts.fontItems">

                    <style ng-if="item.fontType == 'google'">
                    {{item.font.fontImport}}
                    </style>
                    <style ng-if="item.fontType == 'adobe' && item.adobeFontConfirmed">
                    {{item.adobeFontImport}}
                    </style>
                    <style ng-if="item.fontType == 'custom' && item.customFontConfirmed">
                    {{item.customFontImport}}
                    </style>

                    <!-- Accordion tab -->
                    <div class="usn-accordion-tab" ng-class="{'tab-closed' : item.tabStyle, 'tab-open' : !item.tabStyle}">

                        <!-- Font name -->
                        <a ng-click="item.tabStyle = !item.tabStyle">
                            <span style="{{item.fontType == 'adobe' && item.adobeFontConfirmed ? 'font-family: ' + item.adobeFontFamily + '!important; font-weight: ' + item.adobeFontWeight + '!important; font-style: ' + item.adobeFontStyle + '!important;' : item.fontType == 'custom' && item.customFontConfirmed ? 'font-family: \'' + item.customFontName + '\', ' + item.customFontStack + '!important;' : 'font-family: \'' + item.font.fontName + '\', ' + item.font.fontCategory + '!important; font-weight: ' + item.fontWeight + '!important; font-style: ' + item.fontStyle.styleName + '!important;'}}">{{(item.fontType == 'adobe' && item.adobeFontConfirmed) ? item.adobeFontFamily + ', ' + item.adobeFontStyle + ', ' + item.adobeFontWeight : (item.fontType == 'adobe' && !item.adobeFontConfirmed) ? "ADOBE FONT NOT CONFIRMED" : (item.fontType == 'custom' && item.customFontConfirmed) ? '\'' + item.customFontName + '\', ' + item.customFontStack  : (item.fontType == 'custom' && !item.customFontConfirmed) ? "CUSTOM FONT NOT CONFIRMED" : item.font.fontFamily + ', ' + item.fontStyle.styleName + ', ' + item.fontWeight}}</span>
                        </a>
                        <!--// Font name -->
                        <!-- Accordion controls -->
                        <div class="usn-accordion-controls">

                            <div class="usn-move">
                                <svg version="1.1" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                    <path d="m48.438 0.78125c-0.3125 0-0.66406 0.17578-0.97656 0.48828l-12.109 16.211c-0.625 0.625-0.35156 1.2695 0.58594 1.2695h7.8125v25h-25v-7.8125c0-0.9375-0.64453-1.2109-1.2695-0.58594l-16.211 12.109c-0.625 0.625-0.625 1.3281 0 1.9531l16.211 12.109c0.625 0.625 1.2695 0.35156 1.2695-0.58594v-7.8125h25v25h-7.8125c-0.9375 0-1.2109 0.64453-0.58594 1.2695l12.109 16.211c0.625 0.625 1.3281 0.625 1.9531 0l12.109-16.211c0.625-0.625 0.35156-1.2695-0.58594-1.2695h-7.8125v-25h25v7.8125c0 0.9375 0.64453 1.2109 1.2695 0.58594l16.211-12.109c0.625-0.625 0.625-1.3281 0-1.9531l-16.211-12.109c-0.625-0.625-1.2695-0.35156-1.2695 0.58594v7.8125h-25v-25h7.8125c0.9375 0 1.2109-0.64453 0.58594-1.2695l-12.109-16.211c-0.3125-0.3125-0.66406-0.48828-0.97656-0.48828z"></path>
                                </svg>
                                <span>Move</span>
                            </div>
                            <div class="usn-delete" ng-click="removeFontItem($index)">
                                <svg version="1.1" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                    <g>
                                        <path d="m69.5 27.906-1.8516 50.008c-0.03125 0.85938-0.78516 1.5859-1.6484 1.5859h-32c-0.85938 0-1.6172-0.73047-1.6484-1.5859l-1.8516-50.008c-0.054688-1.3789-1.2109-2.457-2.5938-2.4062-1.3789 0.054688-2.457 1.2109-2.4062 2.5938l1.8555 50.008c0.12891 3.5391 3.0938 6.3984 6.6445 6.3984h32c3.5508 0 6.5156-2.8516 6.6445-6.3984l1.8555-50.008c0.050781-1.3828-1.0273-2.5391-2.4062-2.5938-1.3828-0.050781-2.5391 1.0273-2.5938 2.4062z"></path>
