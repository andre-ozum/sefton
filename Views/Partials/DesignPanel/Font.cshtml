                            <!--// Font type item -->
                            <!-- Font type item -->
                            <label class="usn-font-type-item">
                                <input name="taboptions_{{$index}}" type="radio" value="adobe" ng-model="item.fontType" ng-click="updateFontFields($index,$event)">
                                <span></span>
                                <div class="inner">
                                    <svg xmlns="http://www.w3.org/2000/svg" fill="#000000" viewBox="0 0 150 150.2"><g id="Layer_3_1_"><path id="Layer_3" d="M0 .2h150v150H0V.2z" fill="none" /></g><g id="adobe"><path class="st1" d="M92.5 18.1l46.7 111.8V18.1zM13 18.1v111.8L59.7 18.1zM55.7 107.4h21.8l8.9 22.5h19.4L76.1 59.3z" /></g></svg>
