                        <!--// Font types -->
                        <!-- Form Group -->
                        <div class="usn-form-group">

                            <!-- Input Row -->
                            <div class="usn-input-row" ng-show="item.fontType == 'system' || item.fontType == 'google'">
                                <!-- Input Col -->
                                <div class="usn-input-col usn-input-col-full">

                                    <label class="usn-label">Font family</label>
                                    <div class="usn-input">
                                        <div class="usn-input-wrap usn-input-select">
                                            <select class="usn-form-control font-dropdown" ng-model="item.font" ng-change="fontFamilyChange($index);updateTypographyFontDropdowns($index,item.fontType)" ng-options="font as font.fontFamily for font in getFonts(item.fontType) track by font.fontFamily">
                                            </select>
                                        </div>
                                    </div>
                                    <span class="note usn-note">Select your font family from the list.</span>

                                </div>
                                <!--// Input Col -->
                                <!-- Input Col -->
                                <div class="usn-input-col usn-input-col-full">

                                    <label class="usn-label">Font style</label>
                                    <div class="usn-input">
                                        <div class="usn-input-wrap usn-input-select">
                                            <select class="usn-form-control font-dropdown" ng-model="item.fontStyle" ng-change="fontStyleChange($index);updateTypographyFontDropdowns($index,item.fontType)" ng-options="style as style.styleName for style in item.font.fontStyle track by style.styleName">
                                            </select>
                                        </div>
                                    </div>
                                    <span class="note usn-note">Select the style of your font.</span>

                                </div>
                                <!--// Input Col -->
                                <!-- Input Col -->
                                <div class="usn-input-col usn-input-col-full">

                                    <label class="usn-label">Font weight</label>
                                    <div class="usn-input">
                                        <div class="usn-input-wrap usn-input-select">
                                            <select class="usn-form-control font-dropdown" ng-model="item.fontWeight" ng-change="updateTypographyFontDropdowns($index,item.fontType)" ng-options="w for w in item.fontStyle.styleWeight">
                                            </select>
                                        </div>
                                    </div>
                                    <span class="note usn-note">Select the weight of your font.</span>

                                </div>
                                <!--// Input Col -->
                            </div>
                            <!--// Input Row -->
                            <!-- Input Row -->
                            <div ng-show="item.fontType == 'adobe'">

                                <div class="usn-input-row" ng-show="!item.adobeFontConfirmed">

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Web Project ID</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input class="usn-form-control" type="text" ng-model="item.adobeProjectId" ng-change="validateAdobeFields($index)" />
                                            </div>
                                        </div>
                                        <span class="note usn-note">Please enter the web project ID provided by Adobe Fonts.</span>
                                    </div>

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font family</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input class="usn-form-control" type="text" ng-model="item.adobeFontFamily" ng-change="validateAdobeFields($index)">
                                            </div>
                                        </div>
                                        <span class="note usn-note">Enter the font family provided by Adobe Fonts.</span>
                                    </div>


                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font style</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input type="text" class="usn-form-control" ng-model="item.adobeFontStyle" ng-change="validateAdobeFields($index)">
                                            </div>
                                        </div>
                                        <span class="note usn-note">Enter the font style provided by Adobe Fonts.</span>
                                    </div>

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font weight</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input type="text" class="usn-form-control" ng-model="item.adobeFontWeight" ng-change="validateAdobeFields($index)">
                                            </div>
                                        </div>
                                        <span class="note usn-note">Enter the font weight provided by Adobe Fonts.</span>
                                    </div>

                                    <div class="usn-alert usn-warning usn-alert-small">Your 'Adobe Font' will not work against your Typography until you confirm your settings.</div>
                                    <div class="usn-alert usn-danger usn-alert-small" ng-show="item.adobeConfirmationError">The Adobe font with Web Project ID: <strong>{{item.adobeProjectId}}</strong> cannot be found. Please check your settings are correct and try again.</div>

                                </div>
                                <div class="usn-input-row usn-confirmed-adobe-font" ng-show="item.adobeFontConfirmed">

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Web Project ID</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">{{item.adobeProjectId}}</div>
                                        </div>
                                                                        
                                    </div>

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font family</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">{{item.adobeFontFamily}}</div>
                                        </div>
                                                                        
                                    </div>

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font style</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">{{item.adobeFontStyle}}</div>
                                        </div>
                                                                        
                                    </div>
                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font weight</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">{{item.adobeFontWeight}}</div>
                                        </div>
                                                                        
                                    </div>

                                </div>

                                <div class="buttons">
                                    <button ng-show="!item.adobeFontConfirmed" ng-class="{'usn-btn usn-btn-ready' : !item.adobeButtonDisabled, 'usn-btn usn-btn-disabled' : item.adobeButtonDisabled}" ng-click="confirmAdobeFontSettings($index,$event)" ng-disabled="item.adobeButtonDisabled">Confirm Adobe Font Settings</button>
                                    <button ng-show="item.adobeFontConfirmed" ng-click="resetAdobeFontSettings($index,$event)" class="usn-btn usn-btn-default">Reset Adobe Font Settings</button>
                                </div>

                            </div>
                            <!--// Input Row -->
                            <!-- Input Row -->
                            <div ng-show="item.fontType == 'custom'">

                                <div class="usn-input-row" ng-show="!item.customFontConfirmed">

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Woff2 file location</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input class="usn-form-control" type="text" ng-model="item.customFileWoff2" ng-change="validateCustomFields($index)" />
                                            </div>
                                        </div>
                                        <span class="note usn-note">Copy the URL of the woff2 font file uploaded to your media section. For example: /media/*****/*****.woff2</span>
                                    </div>

                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font name</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input class="usn-form-control" type="text" ng-model="item.customFontName" ng-change="validateCustomFields($index)">
                                            </div>
                                        </div>
                                        <span class="note usn-note">Enter the font name of your custom font.</span>
                                    </div>


                                    <div class="usn-input-col usn-input-col-full">
                                        <label class="usn-label">Font stack</label>
                                        <div class="usn-input">
                                            <div class="usn-input-wrap usn-input">
                                                <input type="text" class="usn-form-control" ng-model="item.customFontStack" ng-change="validateCustomFields($index)">
                                            </div>
                                        </div>
                                        <span class="note usn-note">Enter the font stack of your custom font.</span>
                                    </div>

                                    <div class="usn-alert usn-warning usn-alert-small">Your 'Custom Font' will not appear in Typography until you confirm your settings.</div>
                                    <div class="usn-alert usn-danger usn-alert-small" ng-show="item.customFontConfirmationError">Your custom font file was not found. Please check the woff2 file location you entered is correct and try again.</div>

